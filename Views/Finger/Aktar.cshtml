@model test.Models.personelgenel
@{
    if ((string)Session["statusname"] != "Admin")
    {
        if ((string)Session["statusname"] != "Master")
        {
            Response.Redirect("~/Login");
        }
    }
    Layout = "~/Views/Shared/_Layout1.cshtml";
}
<title>İçe Aktar</title>
@using (Html.BeginForm("Aktar", "Finger", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <form>
        <div class="row">
            <div class="col-md-4">
                <div class="card card-warning">
                    <div class="card-header p-0">
                        <ul class="nav nav-pills">
                            <li class=" nav-link"><label>İçe Aktar</label></li>
                        </ul>
                    </div><!-- /.card-header -->
                    <div class="row">
                        <div class="card-body">
                            <div class="tab-pane" id="settings">
                                <div class="form-horizontal">
                                    <div class="input-group input-group-sm">
                                        <label class="col-sm-3 col-form-label">Dosya Seç</label>
                                        <div class="col-sm-8">
                                            <div class="input-group">
                                                <div class="custom-file">
                                                    <input type="file" name="file" value="Resim Seç" id="file" multiple>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                    <div class="form-group">
                                        <div class="col-md-offset-2 ">
                                            <input type="submit" value="Yükle" class="btn btn-info">
                                        </div>
                                    </div>
                                </div>
                                <label style="color:red;font-size:14px">@ViewBag.durum</label>
                            </div>
                            <!-- /.tab-pane -->
                            <!-- /.tab-content -->
                        </div><!-- /.card-body -->
                    </div>
                </div>
            </div>
        </div>
    </form>
}
